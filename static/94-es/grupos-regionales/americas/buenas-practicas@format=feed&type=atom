&lt;/td&gt;
&lt;td style=&quot;width: 40px; padding: 0cm;&quot;&gt;
&lt;p class=&quot;MsoNormal&quot; style=&quot;line-height: 13.5pt;&quot;&gt;&lt;span style=&quot;font-size: 9.0pt; font-family: 'Arial',sans-serif;&quot;&gt;2015&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td style=&quot;width: 191px; padding: 0cm;&quot;&gt;
&lt;p class=&quot;MsoNormal&quot; style=&quot;line-height: 13.5pt;&quot;&gt;&lt;span style=&quot;font-size: 9.0pt; font-family: 'Arial',sans-serif;&quot;&gt;&lt;a href=&quot;http://insarag.org/images/stories/Americas_good_practices/INSARAG_Americas_Chile_buena_practica_GTO_USAR_2015.pdf&quot;&gt;&lt;span style=&quot;color: #666666;&quot;&gt;GTO-USAR&lt;/span&gt;&lt;/a&gt;&lt;/span&gt;&lt;/p&gt;
